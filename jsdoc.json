{
	"opts": {
		"destination": "./doc",
		"package": "./package.json"
	},
	"source": {
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [
		"node_modules/jsdoc-autoprivate/autoprivate.js"
	],
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	}
}